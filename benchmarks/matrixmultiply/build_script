#!/bin/bash
source ./build.config

CURR_DIR=$(pwd)

mkdir ./results
cd $BUILD_DIR/$dir;

#First build the full circuit
rm -r -f db/ incremental_db/;
rm top*;
make;
make p;
make f;
cp top.fit.rpt $CURR_DIR/results/MMM.full.rpt

#Now build the StitchUp circuit  
rm -r -f db/ incremental_db/;
rm top*;
make STITCHUP=1;
make p;
make f;
cp top.fit.rpt $CURR_DIR/results/MMM.stitchedup.rpt


